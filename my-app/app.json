{
	"expo": {
		"name": "Multibrain",
		"slug": "my-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.PNG",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.PNG",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"buildNumber": "1.0.0"
		},
		"android": {
			"versionCode": 1,
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.PNG",
				"backgroundColor": "#FFFFFF"
			},
			"permissions": ["android.permission.RECORD_AUDIO"],
			"package": "com.gykora.myapp"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			[
				"expo-image-picker",
				{
					"cameraPermission": "Necesitas permitir el uso de la camara para continuar"
				}
			]
		],
		"extra": {
			"eas": {
				"projectId": "e71cc97e-39b4-419c-b59e-d61b9b0c9495"
			}
		}
	}
}
